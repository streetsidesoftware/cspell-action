{
  "name": "cspell-action",
  "version": "1.0.0",
  "description": "Source code for GitHub Action: Check Spelling",
  "private": true,
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn compile && yarn webpack && yarn build-cleanup",
    "build-cleanup": "removeNPMAbsolutePaths ../action",
    "webpack": "webpack -c webpack.prod.js",
    "compile": "tsc -p .",
    "smoke-test": "cd .. && env-cmd -f ./fixtures/pull_request.json node ./action-source/debug/test_main.js",
    "watch": "webpack -c webpack.dev.js --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-action.git"
  },
  "keywords": [
    "GitHub",
    "Action",
    "cspell",
    "spelling",
    "checker"
  ],
  "author": "Jason Dent <jason@streetsidesoftware.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-action/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-action#readme",
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/rest": "^18.0.12",
    "@pollyjs/adapter-node-http": "^5.1.0",
    "@pollyjs/core": "^5.1.0",
    "@pollyjs/persister-fs": "^5.0.0",
    "@types/jest": "^26.0.19",
    "@types/minimatch": "^3.0.3",
    "@types/pollyjs__adapter-node-http": "^2.0.1",
    "@types/pollyjs__core": "^4.3.1",
    "@types/pollyjs__persister-fs": "^2.0.1",
    "cspell": "^5.3.1",
    "cspell-glob": "^5.3.1",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "removeNPMAbsolutePaths": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "typescript": "^4.2.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@cspell/cspell-bundled-dicts": "^5.3.0"
  }
}
