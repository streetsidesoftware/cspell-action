# We want to keep the action folder.
!action

# Don't keep map or test files
/action/**/*.map
/action/**/*.test.*
/action/**/*.d.ts

/node_modules
/acton-src/node_modules
/action-src/dist
/debug

# install-state seems to always be changing
install-state.gz
action-src/.yarn/cache
action-src/.yarn/install-state.gz

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Diagnostic reports (https://nodejs.org/api/report.html)
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
lib-cov

# Coverage directory used by tools like istanbul
coverage
*.lcov

# TypeScript cache
*.tsbuildinfo

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# No intelij files
.idea
