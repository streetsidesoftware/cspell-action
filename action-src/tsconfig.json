{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@tsconfig/node24/tsconfig.json",
    "compilerOptions": {
        "noEmit": true,
        "skipLibCheck": true,
        "declaration": true,
        "isolatedDeclarations": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "types": ["node"],
        "outDir": "dist",
        "rootDir": "./src"
    },
    "include": ["src"]
}
