{
    "version": "0.1",
    "words": [
        "camelcase",
        "octokit",
        "repos",
        "shrinkwrap"
    ],
    "ignorePaths": [
        "action/",
        "__snapshots__",
        "node_modules",
        "fixtures/**/*.json",
        "fixtures/sampleCode/*errors/",
        "cspell.json",
        "package-lock.json"
    ],
    "patterns": [
        {
            "name": "Email",
            "pattern": "/<?\\b[\\w.\\-+]{1,64}@\\w{1,63}(\\.\\w{1,63}){1,3}\\b>?/gi"
        }
    ],
    "overrides": [
        {
            "filename": "*.har",
            "languageId": "json",
            "dictionaries": [
                "http",
                "node",
                "typescript"
            ],
            "words": [
                "etag",
                "maxage",
                "ratelimit",
                "nosniff"
            ]
        }
    ],
    "enableFiletypes": [
        "shellscript"
    ]
}
